<%= simple_form_for([@room, @meeting]) do |f| %>
	<%= f.input :meeting_name %>
	<div class="start-time"><%= f.input :start_time, :start_year => 2015, :end_year => 2015 %></div>
	<div class="end-time"><%= f.input :end_time, :start_year => 2015, :end_year => 2015, display: :inline %></div>
	<%= f.button :submit %>
<% end %>
